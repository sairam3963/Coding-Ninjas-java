import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int i = 1;
		int t = n;
		while (i <= n) {
			int j = 1;
			while (j <= t) {
				System.out.print(t);
				j++;
			}
			t--;
			i++;
			System.out.println();
		}

	}

}
