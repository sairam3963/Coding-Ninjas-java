import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int i = 1;
		int space = n;
		while (i <= n) {
			space = space - 1;
			int j = 1;
			while (j <= space) {
				System.out.print(" ");
				j++;
			}
			int k = 1;
			while (k <= i) {
				System.out.print(k);
				k++;
			}
			i++;
			System.out.println();
		}

	}

}
