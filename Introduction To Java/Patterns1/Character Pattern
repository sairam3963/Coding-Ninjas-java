import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int i = 1;
		int val = 0;
		while (i <= n) {
			int j = 1;
			while (j <= i) {
				System.out.print((char) ('A' + val));
				val++;
				j++;
			}
			i++;
			System.out.println();
		}

	}

}
