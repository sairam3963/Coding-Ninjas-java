import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int i = 1;
		int t = 0;
		while (i <= n) {
			int j = 0;
			t = n - i;
			while (j < i) {
				System.out.print((char) ('A' + t));
				t++;
				j++;
			}
			i++;
			System.out.println();
		}

	}

}
