import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int i = 1, a = 1;
		while (i <= n) {
			int sum = (3 * a) + 2;
			if (sum % 4 != 0) {
				System.out.print(sum + " ");
				i++;
			}
			a++;
		}

	}

}
