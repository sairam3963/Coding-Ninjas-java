import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int base = sc.nextInt();
		int expo = sc.nextInt();
		int result = base;

		if (expo >= 1) {
			for (int i = 1; i < expo; i++) {
				result = result * base;
			}
		} else {
			result = 1;
		}
		System.out.println(result);
	}

}
