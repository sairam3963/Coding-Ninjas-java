import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int base = sc.nextInt();
		int expo = sc.nextInt();
		int mul = base;
		if (base == 0) {
			System.out.println(0);
		} else if (expo == 0) {
			System.out.println(1);
		} else {
			for (int i = 1; i < expo; i++) {
				mul = mul * base;
			}
			System.out.println(mul);
		}
	}

}
