import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int val = 2;

		for (int i = 1; i <= n; i++) {
			System.out.print(i);
			for (int j = 1; j < i; j++) {
				val = val + 1;
				System.out.print(val);
			}
			val = i + 1;
			System.out.println();
		}

	}

}
